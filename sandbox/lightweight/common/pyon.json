{
    "container": {
        "sflow": {
            "enabled": true,
            "hsflowd_port": 36343
        },
        "filesystem": {
            "force_clean": false,
            "cache": "/ion-alpha/r2/data/ion/run/cache",
            "temp": "/ion-alpha/r2/data/ion/tmp",
            "run": "/ion-alpha/r2/data/ion/run",
            "log": "/ion-alpha/r2/data/ion/run/log",
            "users": "/ion-alpha/r2/data/ion/usr",
            "resource": "/ion-alpha/r2/data/ion/res",
            "library": "/ion-alpha/r2/data/ion/lib"
        }
    },
    "endpoint": {
        "receive": {
            "timeout": 60
        }
    },
    "server": {
        "amqp": {
            "host": "${rabbitmq_host}",
            "password": "${rabbitmq_password}",
            "username": "${rabbitmq_username}"
        },
        "amqp_priv": {
            "host": "${rabbitmq_host}",
            "password": "${rabbitmq_password}",
            "username": "${rabbitmq_username}"
        },
        "couchdb": {
            "host": "${couchdb_host}",
            "password": "${couchdb_password}",
            "username": "${couchdb_username}"
        },
        "erddap": {
            "type": "erddap",
            "host": "erddap-test.oceanobservatories.org",
            "port": 8080
        },
        "elasticsearch": {
            "host": "bc01.a.oceanobservatories.org"
        },
        "pydap": {
            "host": "pydap.a.oceanobservatories.org",
            "port": 5000
        }
    },
    "system": {
        "auto_bootstrap": false,
        "name": "${sysname}",
        "elasticsearch": false
    },
    "bootstrap": {
        "use_es": false,
        "use_pydap": true
    }
}
