{
  "run_type": "pyon_single",
  "parameters":{
    "rel":{
      "type":"release",
      "name":"resource_registry",
      "version":"0.1",
      "description":"Pyon service deployed by cloudinit.d",
      "ion":"0.0.1",
      "apps":[{
  "version": 0.1, 
  "name": "resource_registry", 
  "processapp": [
    "resource_registry", 
    "ion.services.coi.resource_registry_service", 
    "ResourceRegistryService"
  ], 
  "description": "Resource registry service"
}]
    }
  },
  "server":{
    "amqp":{
      "host": "${rabbitmq_host}",
      "vhost": "/",
      "username": "${rabbitmq_username}",
      "password": "${rabbitmq_password}",
      "exchange": "${exchange_scope}"
    }
  }
}
