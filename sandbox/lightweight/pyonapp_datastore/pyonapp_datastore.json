{
  "run_type": "pyon_single",
  "parameters":{
    "rel":{
      "type":"release",
      "name":"datastore",
      "version":"0.1",
      "description":"Pyon service deployed by cloudinit.d",
      "ion":"0.0.1",
      "apps":[{
  "version": 0.1, 
  "name": "datastore", 
  "processapp": [
    "datastore", 
    "ion.services.coi.datastore_service", 
    "DataStoreService"
  ], 
  "description": "Data store service"
}]
    }
  },
  "server":{
    "amqp":{
      "host": "${rabbitmq_host}",
      "vhost": "/",
      "username": "${rabbitmq_username}",
      "password": "${rabbitmq_password}",
      "exchange": "${exchange_scope}"
    }
  }
}
